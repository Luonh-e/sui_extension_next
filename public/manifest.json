{
  "manifest_version": 3,
  "name": "SQUID",
  "description": "Sui extension using Next.js, Tailwind, and TypeScript",
  "version": "1.0",
  "permissions": ["sidePanel"],
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
